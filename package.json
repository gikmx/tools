{
  "name": "feliz.util",
  "version": "0.0.10",
  "description": "Various (standalone) utilities for feliz.js projects",
  "main": "lib/index.js",
  "author": "Héctor Menéndez <etor@gik.mx>",
  "license": "MIT",
  "analyze": true,
  "keywords": [
    "feliz",
    "helper",
    "util",
    "utilities"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gikmx/feliz.util"
  },
  "bugs": {
    "url": "https://github.com/gikmx/feliz.util/issues"
  },
  "engines": {
    "node": ">=6.2.0"
  },
  "scripts": {
    "start": "npm-watch",
    "docs": "jsdoc2md -t ./template.hbs -d 1 'lib/**/*.js' > README.md",
    "test": "tape ./test/**/*.js | tap-spec",
    "cover": "npm run cover:stats && npm run cover:send",
    "cover:check": "npm run cover:stats && istanbul check-coverage",
    "cover:stats": "istanbul cover tape -- ./test/**/*.js",
    "cover:send": "cat ./coverage/lcov.info | coveralls",
    "precommit": "npm test && npm run cover:check && npm run docs && git add --force ./README.md && feliz.hooks pre-commit"
  },
  "dependencies": {
    "feliz.error": "0.0.3",
    "lodash.merge": "^4.5.1",
    "rxjs": "^5.0.0-beta.10"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "feliz.hooks": "0.0.4",
    "husky": "^0.11.6",
    "istanbul": "^0.4.4",
    "jsdoc-to-markdown": "^1.3.6",
    "npm-watch": "^0.1.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "watch": {
    "test": {
      "patterns": [
        "lib",
        "test"
      ],
      "extensions": "js",
      "quiet": true
    },
    "docs": {
      "patterns": [
        "lib",
        "template.hbs"
      ],
      "extensions": "js",
      "quiet": true
    }
  },
  "dmd": {
    "memberIndex": {
      "minDescendants": 1
    }
  }
}
