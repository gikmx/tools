{
  "name": "feliz.util",
  "version": "0.0.5",
  "description": "Various (standalone) utilities for feliz.js projects",
  "main": "lib/index.js",
  "author": "Héctor Menéndez <etor@gik.mx>",
  "license": "MIT",
  "analyze": true,
  "keywords": [
    "feliz",
    "helper",
    "util",
    "utilities"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gikmx/feliz.util"
  },
  "bugs": {
    "url": "https://github.com/gikmx/feliz.util/issues"
  },
  "engines":{
    "node": ">=6.2.0"
  },
  "scripts": {
    "start": "npm-watch",
    "docs": "jsdoc2md -t ./template.hbs 'lib/**/*.js' > README.md",
    "test": "tape ./test/**/*.js | tap-diff",
    "coverage-pre": "istanbul cover --display details tape ./test/**/*.js",
    "coverage-post": "cat ./coverage/lcov.info | coveralls && rm -Rf coverage",
    "coverage":"npm run coverage-pre && npm run coverage-post",
    "precommit": "npm test && npm run docs && git add ./README.md && feliz.hooks pre-commit"
  },
  "dependencies": {
    "feliz.error": "0.0.2",
    "lodash.merge": "^4.5.0",
    "rxjs": "^5.0.0-beta.10"
  },
  "devDependencies": {
    "feliz.hooks": "0.0.4",
    "husky": "^0.11.6",
    "jsdoc-to-markdown": "^1.3.6",
    "npm-watch": "^0.1.5",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.0"
  },
  "watch": {
    "test": {
      "patterns": [
        "lib",
        "test"
      ],
      "extensions": "js",
      "quiet": true
    },
    "docs": {
      "patterns": [
        "lib",
        "template.hbs"
      ],
      "extensions": "js",
      "quiet": true
    }
  }
}
